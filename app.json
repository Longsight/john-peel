{
  "name": "John Peel",
  "description": "Spotify playlist curator",
  "repository": "https://github.com/Longsight/john-peel",
  "logo": "http://louderthanwar.com/wp-content/uploads/john-peel1.jpg",
  "keywords": ["node", "express", "parse", "spotify"],
  "env": {
    "PARSE_MOUNT": {
      "description": "Configure Parse API route.",
      "value": "/parse"
    },
    "APP_ID": {
      "description": "A unique identifier for your app.",
      "value": "JohnPeel"
    },
    "MASTER_KEY": {
      "description": "A key that overrides all permissions. Keep this secret.",
      "value": "[redacted]"
    },
    "SERVER_URL": {
      "description": "URL to connect to your Heroku instance (update with your app's name + PARSE_MOUNT)",
      "value": "https://john-peel.herokuapp.com/parse"
    }
  },
  "image": "heroku/nodejs",
  "addons": ["mongolab"]
}
